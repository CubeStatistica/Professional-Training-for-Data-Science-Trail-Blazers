dataRaw <- read_csv("Data/Data - DS C1 Course.csv")
head(dataRaw, n = 10)
dataRenamed <- dataRaw %>% rename("Id"= "Student ID", "Date_time" = "Timestamp", "Qr_Uni_Col" = "Are you currently attending University / College?")
# check for renaming
names(dataRenamed[1:10])
dataselected <- select(dataRenamed, Id, Date_time, Qr_Uni_Col)
# MAKING id AS row name
dataselected <- column_to_rownames(dataselected, var= "Id")
head(dataselected)
# Converting time column to date time format
dataselected$Date_time <- lubridate::ymd_hms(dataselected$Date_time)
# Separating date and time into different column
dataselected <- tidyr::separate(dataselected, Date_time, c("Date", "Time"), sep = " ")
dataselected$Date <- lubridate::ymd(dataselected$Date)
head(dataselected)
